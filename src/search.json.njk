---
permalink: search.json
eleventyExcludeFromCollections: true
---
[
{%- for page in collections.all -%}
  {%- if page.data.layout %}
  {
    "url": "{{ site.url }}{{ page.url }}",
    "title": "{{ page.data.title }}",
    "index": "{{ page.templateContent | squash }}"
  }{%- if not loop.last %},{% endif -%}
  {%- endif -%}
{%- endfor -%}
]
